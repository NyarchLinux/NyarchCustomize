<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="Adw" version="1.0"/>
  <object class="GtkGrid" id="gridcontent">
    <property name="row-homogeneous">true</property>
    <property name="column-homogeneous">true</property>
     <child>
       <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
         <layout>
            <property name="column">0</property>
            <property name="row">0</property>
          </layout>
          <child>
            <object class="GtkImage" id="layout0">
              <property name="icon-name">bottom_panel</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l0">
               <property name="halign">center</property>
               <property name="label">Bottom Panel</property>
               <property name="group">l0</property>
             </object>
           </child>
       </object>
      </child>
     <child>
       <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
         <layout>
            <property name="column">1</property>
            <property name="row">0</property>
          </layout>
          <child>
            <object class="GtkImage" id="layout1">
              <property name="icon-name">gnome-layout</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l1">
               <property name="halign">center</property>
               <property name="label">Gnome</property>
               <property name="group">l0</property>
             </object>
           </child>
       </object>
      </child>
      <child>
        <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
         <layout>
            <property name="column">2</property>
            <property name="row">0</property>
          </layout>
          <child>
            <object class="GtkImage" id="layout2">
              <property name="icon-name">top_bar_with_menu</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l2">
               <property name="halign">center</property>
               <property name="group">l0</property>
               <property name="label">Top panel with menu</property>
             </object>
           </child>
       </object>
      </child>
      <child>
        <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
              <layout>
                      <property name="column">0</property>
                      <property name="row">1</property>
              </layout>
          <child>
            <object class="GtkImage" id="layout4">
              <property name="icon-name">top_bar_menu_dock</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l3">
               <property name="halign">center</property>
               <property name="group">l0</property>
               <property name="label">Top panel with menu and dock</property>
             </object>
           </child>
       </object>
      </child>
      <child>
        <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
              <layout>
                      <property name="column">1</property>
                      <property name="row">1</property>
              </layout>
          <child>
            <object class="GtkImage" id="layout5">
              <property name="icon-name">top_bar_dock</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l4">
               <property name="halign">center</property>
               <property name="group">l0</property>
               <property name="label">Top panel with dock</property>
             </object>
           </child>
       </object>
      </child>
      <child>
        <object class="GtkBox">
         <property name="orientation">GTK_ORIENTATION_VERTICAL</property>
         <property name="baseline-position">center</property>
              <layout>
                      <property name="column">2</property>
                      <property name="row">1</property>
              </layout>
          <child>
            <object class="GtkImage" id="layout6">
              <property name="icon-name">top_bar_left_dock</property>
              <property name="pixel-size">200</property>
              <property name="hexpand">true</property>
            </object>
            </child>
            <child>
             <object class="GtkCheckButton" id="l5">
               <property name="halign">center</property>
               <property name="group">l0</property>
               <property name="label">Top panel with left dock</property>
             </object>
           </child>
       </object>
      </child>
  </object>
</interface>
